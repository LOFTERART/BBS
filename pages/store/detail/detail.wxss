.tui-badge-box {
    position: relative;
}

.tui-menu-badge {
    position: absolute;
    top: -8px;
    right: -8px;
}

.tui-msg-badge {
    top: -10px;
}

.tui-icon-up {
    position: relative;
    display: inline-block;
    left: 50%;
