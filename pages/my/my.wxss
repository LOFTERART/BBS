@import "../../app.wxss";
@import "../../iconFont/iconFont.wxss";
.cellTStyle{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 70rpx;
    padding: 10rpx 20rpx;
}

.tui-classify-img {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

#user .banner{position:relative;padding:30px 0;background-color: #C9A063}
#user .banner .ban-item-l{display:flex;align-items:center;justify-content:center}
#user .banner .ban-item-l .user-avatar{width:78px;height:78px;border-radius:50%;margin-right:15px}
#user .banner .ban-item-l .user-avatar image{width:100%;height:100%;border-radius:50%}
#user .banner .ban-item-l .user-text{font-size:0}
#user .banner .ban-item-l .user-text .nick-name{font-size:16px;color:#FFF;padding-bottom:8px}
#user .banner .unlogin{margin:0 auto;text-align:center;height:125px;line-height:125px;color:#FFF;font-size:38rpx}
#user .all-orders,#user .info-detail{background-color:#fff;margin-bottom:12px}
#user .banner .button{opacity:0;position:absolute;top:0;bottom:0;width:100%}
#user .all-orders .order-header{display:flex;align-items:center;justify-content:space-between;padding:11px 15px 9px}
#user .all-orders .order-header .order-text{font-size:14px;color:#000;line-height:20px;font-weight:700}
#user .all-orders .order-header .iconfont{font-size:12px;color:#B3B3B3}#user .all-orders .order-content{display:flex;align-items:center;justify-content:center;padding:20px 0 10px}
#user .all-orders .order-content .item{flex:1;text-align:center;font-size:12px;color:#6A6A6A;position:relative}#user .all-orders .order-content .item .iconfont{font-size:20px;color:#6A6A6A}#user .all-orders .order-content .item .position-num{position:absolute;top:-5px;right:23px;width:16px;height:16px;line-height:16px;text-align:center;background-color:#fff;border:1px solid #C9A063;color:#C9A063;font-size:10px;border-radius:50%;overflow:hidden;white-space:nowrap}#user .info-detail{display:flex;padding:15px 0;align-items:center;justify-content:center}#user .info-detail .item{flex:1;text-align:center;font-size:12px;color:#6A6A6A;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}#user .info-detail .item .number{font-size:16px;color:#C9A063;line-height:15px;padding-bottom:7px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}